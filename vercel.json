{
  "version": 2,
  "framework": "static",
  "installCommand": "",
  "buildCommand": "",
  "outputDirectory": ".gh-pages",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://my-store-backend-production.up.railway.app/api/:path*"
    },
    {
      "source": "/uploads/:path*",
      "destination": "https://my-store-backend-production.up.railway.app/uploads/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
