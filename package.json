{
  "$schema": "./package.schema.json",
  "name": "my-store",
  "private": true,
  "homepage": "",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "roadmap:sync": "node ./scripts/sync-roadmap-issues.js",
    "project:setup": "node ./scripts/setup-project-board.js",
    "dev": "npm run dev -w client",
    "start:simple": "node server/simple.js",
    "dev:simple": "node server/simple.js",
    "start:server": "node server/index.js",
    "dev:server": "node server/index.js",
    "dev:server:admin": "cross-env ALLOW_DEV_HEADERS=true PORT=8842 node server/index.js",
    "build": "npm run build -w client",
    "server": "node server/index.js",
    "start": "node server/index.js",
    "start:prod": "cross-env NODE_ENV=production SERVE_CLIENT=true node server/index.js",
    "prestart": "prisma migrate deploy",
    "postinstall": "prisma generate",
    "pm2:start": "pm2 start ecosystem.config.js && pm2 save",
    "pm2:restart": "pm2 restart my-store-server",
    "pm2:logs": "pm2 logs my-store-server",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prisma:validate": "npx prisma validate",
    "migrate:deploy": "prisma migrate deploy",
    "ci:verify": "npm run -s typecheck && npm run -s build && npm run -s prisma:validate",
    "preview": "vite preview",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:studio": "npx prisma studio",
    "db:seed": "node server/db/seed.js",
    "db:seed:subscriptions": "node scripts/seed-subscriptions.js",
    "db:reset": "node scripts/db-reset.js",
    "db:sql": "node server/scripts/applySql.js scripts/Store_DB.sql",
    "admin:create": "node server/scripts/createAdmin.js",
    "login:verify": "node server/scripts/verifyLogin.js",
    "user:show": "node server/scripts/showUser.js",
    "users:list": "node server/scripts/listUsers.js",
    "db:check": "node server/scripts/dbCheck.js",
    "auth:reset": "node server/scripts/resetPassword.js",
    "dev:debug": "cross-env DEBUG_LOGIN=1 DEBUG_PRODUCTS=1 node server/index.js",
    "products:generate": "node server/scripts/generateProducts.js",
    "products:import": "node server/scripts/importProducts.js server/data/realProducts.sample.json",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "dev:all": "npm-run-all -p dev:server dev:client",
    "dev:init": "npm run db:generate && npm run db:push && npm run db:seed",
    "dev:server:dbless": "cross-env QUICK_START_DB=1 ALLOW_INVALID_DB=true node server/index.js",
    "preview:all": "npm-run-all -p server preview",
    "check:dead": "knip --strict || exit 0",
    "check:deps": "depcheck --skip-missing",
    "check:empty-pages": "node scripts/empty-pages-report.mjs",
    "icons:generate": "node scripts/generate-icons.mjs",
    "test:api": "node scripts/tests/api-smoke.mjs",
    "test:sse": "node scripts/tests/order-delivery-sse.mjs",
    "test:all": "npm run -s test:api && npm run -s test:sse",
    "test:payments:e2e": "node scripts/tests/payments-invoices-e2e.mjs",
    "test:bank:e2e": "node scripts/tests/bank-invoices-e2e.mjs",
    "test:paypal:e2e": "node scripts/tests/paypal-invoices-e2e.mjs",
    "dev:server:test": "cross-env PORT=4010 node server/index.js",
    "test:api:8829": "cross-env API_BASE=http://localhost:8829 node scripts/tests/api-smoke.mjs",
    "test:sse:8829": "cross-env API_BASE=http://localhost:8829 node scripts/tests/order-delivery-sse.mjs",
    "test:all:8829": "npm run -s test:api:8829 && npm run -s test:sse:8829",
    "dev:client": "npm run dev -w client",
    "build:client": "npm run build -w client",
    "preview:client": "npm run preview -w client",
    "test:unit": "vitest -w client",
    "test:unit:run": "vitest run -w client",
    "test:ui": "playwright test -c client/playwright.config.ts",
    "test:ui:headed": "playwright test -c client/playwright.config.ts --headed",
    "test:integration": "npm run -s dev:server:test & timeout 3 > NUL & cross-env PLAYWRIGHT_BASE_URL=http://localhost:4010 npm run -s test:ui",
    "diag": "node server/scripts/diagnostics.mjs",
    "diag:8842": "cross-env PORT=8842 node server/scripts/diagnostics.mjs",
    "diag:8843": "cross-env PORT=8843 node server/scripts/diagnostics.mjs",
    "package:theme": "node scripts/package-theme.js"
  },
  "prisma": {
    "seed": "node ./server/db/seed.js"
  },
  "dependencies": {
    "@apollo/server": "^4.11.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/cairo": "^5.2.7",
    "@hookform/resolvers": "^5.2.2",
    "@mui/material": "^7.3.4",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-stringify": "^6.5.2",
    "dotenv": "^16.6.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.4.0",
    "framer-motion": "^12.23.24",
    "graphql": "^16.11.0",
    "helmet": "^7.2.0",
    "i18next": "^25.5.3",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.544.0",
    "multer": "^2.0.2",
    "mysql2": "^3.15.2",
    "pdfkit": "^0.15.0",
    "pino": "^9.5.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^11.2.2",
    "puppeteer": "^23.11.1",
    "qrcode": "^1.5.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.65.0",
    "react-i18next": "^16.0.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^6.30.1",
    "redis": "^5.9.0",
    "sharp": "^0.33.5",
    "speakeasy": "^2.0.0",
    "stripe": "^19.3.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-rtl": "^0.9.0",
    "twilio": "^4.22.0",
    "use-debounce": "^10.0.6",
    "ws": "^8.18.3",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.49.0",
    "@prisma/client": "^6.19.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "chrome-launcher": "^1.2.1",
    "cross-env": "^10.1.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "knip": "^5.27.0",
    "lighthouse": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prisma": "^6.19.0",
    "sass-embedded": "^1.93.2",
    "tailwindcss": "^4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.9.2",
    "vite": "^7.1.10",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^4.0.8"
  },
  "workspaces": [
    "client",
    "server",
    "packages/*"
  ]
}
